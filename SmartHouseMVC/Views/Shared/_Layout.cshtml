@using SmartHouseMVC.Models.clas
@model IDictionary<int, Technics>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div id="Left">
        <h3>Здесь можно добавлять раные устройства</h3>
        @using (Html.BeginForm("Add", "My"))
        {
            @Html.DropDownList("TecnicaType", (IEnumerable<SelectListItem>)ViewBag.TecnicaList)
            <input type="submit" value="Добавить">
        }
    </div>
    <div id="Right">
        <h3>Тут ещё что то будет</h3>
    </div>
    <div id="Center">
        <h2>Smart House for Smart people</h2>
        @RenderBody()
        <hr />
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
